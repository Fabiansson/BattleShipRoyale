  "dependencies": {
    "bluebird": "^3.7.2",
    "concurrently": "^5.1.0",
    "connect-redis": "^4.0.4",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-socket.io-session": "^1.3.5",
    "redis": "^3.0.2",
    "socket.io": "^2.3.0"
  }
